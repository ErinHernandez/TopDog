    "babel-plugin-polyfill-corejs3": "^0.13.0",
    "cypress": "^14.5.0",
    "eslint": "9.33.0",
    "eslint-config-next": "^16.0.0",
    "firebase-tools": "^14.11.1",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.2.0",
    "node-mocks-http": "^1.15.1",
    "postcss": "^8.4.27",
    "puppeteer": "^24.17.0",
    "rimraf": "^6.0.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.9.3",
    "vercel": "^32.7.2"
  }
}
